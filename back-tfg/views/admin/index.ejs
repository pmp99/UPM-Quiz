<h1>
    <%= title || "Usuarios" %>:
</h1>

<table>
    <% for (var i in users) { %>
    <% var user = users[i]; %>
    <tr>
        <td><%= user.username %></td>
        <% if (!user.isAdmin) {%><td><a href="/admin/view/<%=user.id%>?_method=DELETE">Delete</a></td><%}%>
        <td><a href="/admin/view/<%= user.id %>">View</a></td>
    </tr>
    <% } %>
    
</table>

<a href="/">Back</a>